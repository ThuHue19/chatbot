{
  "table": "PAKH_CA_NHAN",
  "description": "Chi tiết xử lý phản ánh theo từng cá nhân. Mỗi bản ghi gắn với một phản ánh từ bảng PAKH và người xử lý.",
  "columns": {
    "ID": {
      "type": "NUMBER",
      "description": "Mã định danh của bản ghi"
    },
    "TRUNG_TAM": {
      "type": "VARCHAR2",
      "description": "Trung tâm xử lý phản ánh"
    },
    "PHONG_BAN": {
      "type": "VARCHAR2(100 BYTE)",
      "description": "Phòng ban phụ trách"
    },
    "TO_NHOM": {
      "type": "VARCHAR2(100 BYTE)",
      "description": "Tổ/nhóm kỹ thuật"
    },
    "CA_NHAN": {
      "type": "VARCHAR2(1000 BYTE)",
      "description": "Tên người xử lý (cá nhân)"
    },
    "NGAY": {
      "type": "DATE",
      "description": "Ngày ghi nhận"
    },
    "MA_TINH": {
      "type": "VARCHAR2",
      "description": "Mã tỉnh"
    },
    "MA_LOAI_PA": {
      "type": "VARCHAR2",
      "description": "Mã loại phản ánh"
    },
    "FO_BO": {
      "type": "VARCHAR2",
      "description": "Lớp dịch vụ (FO/BO)"
    },
    "LOAI_THUE_BAO": {
      "type": "VARCHAR2",
      "description": "Loại thuê bao (KHDN, KHCN, VIP)"
    },
    "TRANG_THAI": {
      "type": "VARCHAR2",
      "description": "Trạng thái của phản ánh (TU_CHOI, HOAN_THANH, DONG, DANG_XU_LY, DA_XU_LY)"
    },
    "IS_TICKET": {
      "type": "VARCHAR2",
      "description": "Trạng thái xử lý"
    },
    "NGUYEN_NHAN": {
      "type": "VARCHAR2",
      "description": "Nguyên nhân lỗi"
    },
    "CEN_CODE": {
      "type": "NUMBER",
      "description": "Mã trung tâm kỹ thuật"
    },
    "CA_TRUC": {
      "type": "VARCHAR2",
      "description": "Ca trực (SÁNG, CHIỀU, TỐI)"
    },
    "TL_TIEN_XLY": {
      "type": "NUMBER",
      "description": "Thời lượng chuẩn bị xử lý (phút)"
    },
    "TL_NOC_XLY": {
      "type": "NUMBER",
      "description": "Thời lượng noc xử lý (phút)"
    },
    "TONG_TL_XLY": {
      "type": "NUMBER",
      "description": "Tổng thời gian xử lý (phút)"
    },
    "DANH_GIA_NOC_XLY": {
      "type": "VARCHAR2",
      "description": "Đánh giá noc xử lý"
    },
    "DANH_GIA_TTML_XLY": {
      "type": "VARCHAR2",
      "description": "Đánh giá thông tin mạng lưới xử lý"
    },
    "TL_XU_LY": {
      "type": "NUMBER",
      "description": "Tỷ lệ xử lý"
    },
    "DANH_GIA_DONG": {
      "type": "VARCHAR2",
      "description": "Đánh giá đóng xử lý"
    },
    "MA_HUYEN": {
      "type": "VARCHAR2",
      "description": "Mã huyện"
    },
    "FB_GROUP": {
      "type": "NUMBER",
      "description": "Nhóm phản ánh"
    },
    "LIEN_KET_SU_KIEN": {
      "type": "VARCHAR2",
      "description": "Liên kết sự kiện liên quan đến phản ánh"
    }
  },
  "select_instructions": "khi yêu cầu liệt kê số phản ánh 1 cá nhân từ chối, hãy lấy thông tin từ bảng PAKH, ví dụ như sau SELECT * FROM PAKH_CA_NHAN pcn JOIN PAKH ON PAKH.ID = pcn.ID WHERE pcn.CA_NHAN = 'duc.vole' AND pcn.TRANG_THAI = 'TU_CHOI'",
