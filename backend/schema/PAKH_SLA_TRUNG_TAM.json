    },
    {
      "name": "NGUYEN_NHAN",
      "type": "VARCHAR2"
    },
    {
      "name": "MA_HUYEN",
      "type": "VARCHAR2"
    },
    {
      "name": "FB_GROUP",
      "type": "NUMBER"
    }
  ],
  "aggregate_columns": [
    {
